  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText> SELECT  RaceName.RaceName, MIN(Datediff("yyyy",Participant.DOB,getdate())) AS AGE 
FROM Participant
LEFT JOIN RaceName ON PARTICIPANT.PPSno = RaceName.PPSno
GROUP BY RaceName
ORDER BY RaceName</CommandText>
      </Query>
      <Fields>
        <Field Name="RaceName">
          <DataField>RaceName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="AGE">
          <DataField>AGE</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
